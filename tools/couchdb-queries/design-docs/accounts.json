{"_id": "_design/Accounts_DD",
 "language": "javascript",
 "views":{
   "all":{
      "map":"function(doc){if (doc.docType=='account'){emit(null,1);}}",
      "reduce": "_count"
   },
   "agency":{
      "map":"function(doc){if (doc.docType=='account'){emit(doc.parentAgency, 1);}}",
      "reduce":"_count"
   },
   "agency_date":{
      "map":"function(doc){var record_date = doc.date_;var record_date = record_date.substring(0,10);if (doc.docType=='account'){emit([doc.parentAgency,record_date], 1);}}",
      "reduce":"function(keys, values){var sum=0;for(var idx in values) {sum = sum + values[idx];} return sum;} "
   },
   "date":{
      "map":"function(doc){var record_date = doc.date_;var record_date = record_date.substring(0,10);if (doc.docType=='account'){emit(record_date, 1);}}",
      "reduce":"_count"
   },
   "statusUpdates_agency_alltime":{
      "map":"function(doc){if (doc.docType=='account'){emit(doc.parentAgency, parseInt(doc._rev.substring(0,1)));}}",
      "reduce":"_sum"
   }
 }
}
